{
	"name": "flowbite-svelte-starter",
	"version": "0.1.3",
	"description": "Flowbite-Svelte starter kit",
	"author": {
		"name": "Shinichi Okada",
		"email": "connect@codewithshin.com",
		"url": "https://blog.codewithshin.com"
	},
	"bugs": "https://github.com/shinokada/flowbite-svelte-starter/issues",
	"homepage": "https://github.com/shinokada/flowbite-svelte-starter",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.39.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.27.3",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"@types/cookie": "^0.5.1",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flowbite": "^2.0.0",
		"flowbite-svelte": "^0.44.19",
		"flowbite-svelte-blocks": "^0.5.1",
		"flowbite-svelte-icons": "^0.4.5",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.2",
		"svelte-check": "^3.4.3",
		"svelte-heros": "^5.2.0",
		"tailwindcss": "^3.3.5",
		"tailwind-merge": "^2.0.0",
		"tslib": "^2.6.2",
		"typescript": "^5.0.0",
		"vite": "^4.5.0",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"sveltekit",
		"flowbite",
		"flowbite-svelte",
		"tailwindcss"
	],
	"engines": {
		"npm": ">=7.0.0",
		"node": ">=16.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shinokada/flowbite-svelte-starter.git"
	},
	"dependencies": {
		"flowbite-svelte-icons": "^0.4.5",
		"tailwind-merge": "^2.0.0"
	}
}
